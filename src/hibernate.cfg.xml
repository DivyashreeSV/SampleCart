<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration SYSTEM "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">


<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password">Saras@1234</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/spring_security_custom_user_demo</property>
		<property name="show_sql">false</property>
		<property name="format_sql">false</property>
		<mapping class="com.ipl.entity.Login" />
		<mapping class="com.ipl.entity.Items" />
		<mapping class="com.ipl.entity.OrdersRecord" />
		<mapping class="com.ipl.entity.OrderItem" />
	</session-factory>
</hibernate-configuration>
